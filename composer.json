{
    "name": "mhasnainjafri/restapikit",
    "description": "Unlock the full potential of your REST API development with RestApiKit. This powerful toolkit offers a variety of utilities to simplify the process of creating, testing, and managing REST APIs. From automatic documentation generation to built-in validation, RestApiKit provides a seamless workflow for developers looking to build reliable and scalable APIs with ease. Focus more on the logic, and let RestApiKit handle the heavy lifting.",
    "keywords": [
        "mhasnainjafri",
        "restapikit"
    ],
    "homepage": "https://github.com/mhasnainjafri/restapikit",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Muhammad Hasnain",
            "email": "mhasnainjafri51214@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mhasnainjafri\\RestApiKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mhasnainjafri\\RestApiKit\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mhasnainjafri\\RestApiKit\\RestApiKitServiceProvider"
            ],
            "aliases": {
                "RestApiKit": "Mhasnainjafri\\RestApiKit\\RestApiKitFacade"
            }
        }
    }
}
